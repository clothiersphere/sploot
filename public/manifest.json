{
  "short_name": "Sploot",
  "name": "Sploot Megalinks Base64 Decoder ",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
    "matches": ["http://*/*"],
    "js": ["onCopy.js"]
    }
  ]
  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite"
  ]
}
